{
  "name": "food-delivery",
  "version": "1.0.0",
  "description": "A React food delivery web app is a web app that uses the React framework to create a user-friendly interface for ordering food from restaurants.",
  "main": "server.js",
  "engines": {
    "node": "18.18.0",
    "npm": "9.8.1"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "vercel-build": "npm install --prefix client && npm run build --prefix client",
    "build": "cross-env CI=false npm run build-prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kumilachew-g/food-delivery.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kumilachew-g/food-delivery/issues"
  },
  "homepage": "https://github.com/Kumilachew-g/food-delivery#readme",
  "dependencies": {
    "express": "^4.18.2",
    "mongodb": "^2.2.36",
    "mongoose": "^7.4.2",
    "nodemon": "^3.0.1",
    "router": "^1.3.8",
    "stripe": "^13.4.0",
    "uuid": "^9.0.0"
  }
}
